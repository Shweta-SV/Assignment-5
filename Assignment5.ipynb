   "source": [
    "import numpy as np\n",
    "#QR Decomposition of Matrix V\n",
    "q = ([0.9701, -0.2425],[-0.2425, -0.9701])\n",
    "r = ([6.1846, -0.7276],[0, 3.2742])\n",
    "print('Q =',q)\n",
    "print('\\nR =',r)\n",
    "#V = QR\n",
    "V = np.dot(q,r)\n",
    "print('\\nV =',V)"
   ]
